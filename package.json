{
  "name": "agenc-operator",
  "version": "1.0.0",
  "description": "Tetsuo - Cyberpunk voice operator for AgenC Solana protocol",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:desktop": "vite --mode desktop",
    "dev:mobile": "vite --mode mobile",
    "build": "tsc && vite build",
    "build:desktop": "tsc && vite build --mode desktop",
    "build:mobile": "tsc && vite build --mode mobile",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:desktop": "tauri build",
    "tauri:build:mobile": "tauri android build --target aarch64",
    "lint": "eslint src --ext ts,tsx",
    "typecheck": "tsc --noEmit",
    "optimize-model": "gltf-transform optimize public/models/agencfinalformr.glb public/models/agencfinalformr.glb --compress draco --texture-compress false && gltf-transform webp public/models/agencfinalformr.glb public/models/agencfinalformr.glb --quality 90"
  },
  "dependencies": {
    "@react-three/drei": "^9.114.0",
    "@react-three/fiber": "^8.17.10",
    "@react-three/postprocessing": "^2.19.1",
    "@solana-mobile/wallet-adapter-mobile": "^2.2.5",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/web3.js": "^1.98.4",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "browserslist": "^4.24.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.11.0",
    "postprocessing": "^6.38.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "three": "^0.169.0",
    "wiggle": "^0.0.17",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@gltf-transform/cli": "^4.1.0",
    "@tauri-apps/cli": "^2.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.169.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.24",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
